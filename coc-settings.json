{
	"coc.preferences.enableMessageDialog":true,
	"suggest.noselect": true,
	"diagnostic.errorSign": "✘",
	"diagnostic.hintSign":"",
	"diagnostic.infoSign":"",
	"languageserver": {
		"intelephense": {
			"command": "intelephense",
			"args": ["--stdio"],
			"filetypes": ["php"],
			"initializationOptions": {
				"storagePath": "/tmp/intelephense"
			}
		},
		"haskell": {
			"command": "haskell-language-server-wrapper",
			"args": [
				"--lsp"
			],
			"rootPatterns": [
				".stack.yaml",
				".hie-bios",
				"BUILD.bazel",
				"cabal.config",
				"package.yaml"
			],
			"filetypes": [
				"hs",
				"lhs",
				"haskell"
			]
		},
		"efm": {
			"command": "~/go/bin/efm-langserver",
			"args": [],
			"filetypes": ["vim", "eruby", "markdown", "yaml"]
		},
		"godot": {
			"host": "127.0.0.1",
			"filetypes": ["gd", "gdscript", "gdscript3"],
			"port": 6008
		},
		"csharp": {
			"command": "~/.dotnet/tools/csharp-ls",
			"rootPatterns": [
				"*.csproj",
				"*.sln",
				"git"
			],
			"filetypes": ["cs", "csharp"]
		}
	},
